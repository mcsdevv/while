{
  "name": "while",
  "version": "1.1.1",
  "private": true,
  "description": "Sync Google Calendar events with Notion databases",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcsdevv/while.git"
  },
  "homepage": "https://github.com/mcsdevv/while#readme",
  "bugs": {
    "url": "https://github.com/mcsdevv/while/issues"
  },
  "keywords": [
    "notion",
    "google-calendar",
    "sync",
    "automation",
    "nextjs",
    "vercel",
    "turborepo"
  ],
  "scripts": {
    "build": "turbo build",
    "build:standalone": "tsx scripts/build-standalone.ts",
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@while/web",
    "dev:dashboard": "turbo dev --filter=@while/dashboard",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "format": "biome format --write .",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^25.0.3",
    "tsx": "^4.19.0",
    "turbo": "^2.8.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild",
      "sharp"
    ]
  },
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "geist": "^1.5.1"
  }
}
