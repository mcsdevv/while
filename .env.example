# Notion Configuration
NOTION_API_TOKEN=secret_xxxxxxxxxxxxx
NOTION_DATABASE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Google Calendar Configuration
GOOGLE_CALENDAR_CLIENT_ID=xxxxxxxxxx.apps.googleusercontent.com
GOOGLE_CALENDAR_CLIENT_SECRET=GOCSPX-xxxxxxxxxx
GOOGLE_CALENDAR_REFRESH_TOKEN=1//xxxxxxxxxx
GOOGLE_CALENDAR_CALENDAR_ID=primary

# Authentication (NextAuth + Google OAuth)
NEXTAUTH_SECRET=your-random-secret-here-generate-with-openssl-rand-base64-32
NEXTAUTH_URL=http://localhost:3000
AUTH_GOOGLE_ID=xxxxxxxxxx.apps.googleusercontent.com
AUTH_GOOGLE_SECRET=GOCSPX-xxxxxxxxxx
AUTHORIZED_EMAILS=your-email@gmail.com,another-email@gmail.com

# Webhooks & Admin
WEBHOOK_URL=https://your-domain.vercel.app/api/webhooks/google-calendar
ADMIN_SECRET=your-admin-secret
CRON_SECRET=your-cron-secret

# Optional Configuration
NODE_ENV=development
LOG_LEVEL=info
