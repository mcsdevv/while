{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm turbo build --filter=@notion-gcal-sync/dashboard",
  "outputDirectory": "apps/dashboard/.next",
  "framework": "nextjs",
  "installCommand": "pnpm install",
  "crons": [
    {
      "path": "/api/cron/sync-notion",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/renew-webhook",
      "schedule": "0 12 * * *"
    }
  ]
}
